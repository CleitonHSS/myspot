  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development ROOT_PATH=./ HOST=localhost OUTPUT=standard PORT=8111 mode=development webpack --display-error-details -w --config webpack.config.js",
    "hml": "cross-env NODE_ENV=localhml ROOT_PATH=./ HOST=localhost PORT=8111 webpack --display-error-details -w --config webpack.config.js --hot",
    "build": "cross-env ROOT_PATH=./ HOST=localhost OUTPUT=standard PORT=8111 npm run build:prod",
    "build:prod": "cross-env ROOT_PATH=./ webpack --display-error-details --config webpack.config.js",
    "build:staging": "webpack --display-error-details --config webpack.config.js -p"
